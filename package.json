{
  "name": "data-import",
  "version": "0.0.1",
  "description": "",
  "author": "Volodymyr Kmet",
  "private": true,
  "scripts": {
    "build": "nest build",
    "watch": "nest build --watch",
    "format": "prettier --write \"src/**/*.ts\" \"migrations/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"serverless offline start --stage local\" \"npm run watch\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "deploy:dev": "npm run build && serverless deploy --stage dev",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run build && npm run typeorm -- migration:generate -d dist/src/shared/database/datasource.config.js ./migrations/$npm_config_name",
    "start:migration:dev": "sls invoke -f migration --stage dev",
    "migration:run": "npm run build && npm run typeorm -- migration:run -d dist/src/shared/database/datasource.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-opensearch": "^3.896.0",
    "@aws-sdk/client-rds-data": "^3.896.0",
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-secrets-manager": "^3.896.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "aws-opensearch-connector": "^1.2.0",
    "aws-sdk": "^2.1692.0",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "luxon": "^3.7.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "rxjs": "^7.8.1",
    "stream-chain": "^3.4.0",
    "stream-json": "^1.9.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/aws-lambda": "^8.10.152",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/stream-json": "^1.7.8",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.4.2",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^14.4.0",
    "serverless-offline-watcher": "^1.1.0",
    "serverless-webpack": "^5.15.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
